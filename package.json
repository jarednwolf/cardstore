{
  "name": "deckstack",
  "version": "1.0.0",
  "description": "DeckStack - Stack the deck in your favor with automated card store operations",
  "main": "dist/index.js",
  "scripts": {
    "build": "prisma generate && tsc -p tsconfig.production.json",
    "vercel-build": "prisma generate && tsc -p tsconfig.production.json",
    "postinstall": "prisma generate",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "./scripts/migrate-production.sh",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "tsx src/database/seed.ts",
    "setup": "node scripts/onboarding.js",
    "health": "node scripts/health-check.js",
    "health:json": "node scripts/health-check.js --json",
    "deploy": "./scripts/deploy-production.sh",
    "deploy:railway": "DEPLOY_TARGET=railway ./scripts/deploy-production.sh",
    "deploy:vercel": "DEPLOY_TARGET=vercel ./scripts/deploy-production.sh",
    "deploy:both": "DEPLOY_TARGET=both ./scripts/deploy-production.sh",
    "deploy:supabase": "./scripts/deploy-supabase.sh",
    "docker:build": "docker build -t deckstack .",
    "docker:run": "docker run -p 3000:3000 deckstack",
    "docker:dev": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f app",
    "monetization:setup": "node scripts/setup-stripe-products.js",
    "monetization:test": "node scripts/test-billing.js",
    "monetization:deploy": "npm run build && vercel --prod",
    "stripe:create-products": "node scripts/create-stripe-products.js",
    "stripe:configure": "node scripts/configure-stripe.js",
    "stripe:test-webhook": "stripe listen --forward-to localhost:3005/api/v1/billing/webhook"
  },
  "keywords": [
    "ecommerce",
    "inventory-management",
    "shopify",
    "tcg",
    "trading-cards",
    "multi-channel"
  ],
  "author": "DeckStack Engineering Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@shopify/shopify-api": "^11.14.1",
    "@stripe/stripe-js": "^7.8.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.53.0",
    "@types/ioredis": "^4.28.10",
    "@types/socket.io": "^3.0.1",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nats": "^2.18.0",
    "node-cron": "^3.0.3",
    "socket.io": "^4.8.1",
    "stripe": "^18.4.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.4",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prisma": "^5.7.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
